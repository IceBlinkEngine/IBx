                  "NodeIsActive": true,
                  "NodePortraitBitmap": "",
                  "NodeNpcName": "",
                  "NodeSound": "none",
                  "conversationText": "A thin weedy voice responds. \"You're not tricking me that easily. I'm not opening this door. I'm not!\"",
                  "IsExpanded": true,
                  "subNodes": [
                    {
                      "idNum": 4,
                      "orderNum": 0,
                      "pcNode": true,
